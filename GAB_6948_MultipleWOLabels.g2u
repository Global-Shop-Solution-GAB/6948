Program.Sub.Main.Start
'by Josh Withrow
Function.Intrinsic.UI.UsePixels ' Allows you to use Pixels instead of Twips throughout

V.Local..BulkDeclareDate(dtFrom,dtTo)
V.Local..BulkDeclareString(sSQL)

'*************************************************************************************************
'************* Start Dashboard Template Stuff

'REQUIRED...
'Set list of controls (grids) that should have their preferences saved.  You know, so they grids load the same way the user left them.
'FORMNAME*!*CONTROLNAME*!*GRIDVIEWNAME#$#form*!*gsgridcontrol*!*gridview#$#etc*!*etc*!*etc
'Wash/rinse/repeat for each gridview that you want to store.
'If there is only one gridview to maintain, only supply the single reference.
V.Global.sGridViews.Set("frmMain*!*GsGCData*!*gvData")

'IGNORE...
'Set ARC ID
'Grabs the ARCID from the filename for use with saving/restoring grid preferences.  So make sure you stick with the file naming convention!
F.Intrinsic.Control.CallSub(SetARCID)

'Open DB Connection
F.ODBC.Connection!con.OpenCompanyConnection(500)
F.ODBC.Connection!conc.OpenCommonConnection(500)

'*************************************************************************************************
'************* Start If Launching From SFDC2 Stuff
'If called from SFDC.NET or GUI Populate Hook, change button text on assigned button.
F.Intrinsic.Control.SelectCase(V.Caller.Hook)
	F.Intrinsic.Control.CaseAny(15710,49710)
		F.Intrinsic.Control.CallSub(DynamicallyRenameScriptButton,"sScriptButtonText","WO Labels")
		F.Intrinsic.Control.CallSub(frmMain_UnLoad)
F.Intrinsic.Control.EndSelect

'************* End Dashboard Template Stuff
'*************************************************************************************************

'Everything beyond this point isn't tied to the "template", but is project specific programming.

'Load List of Sentinel Printers...
F.Intrinsic.Control.CallSub(LoadSentinelPrinters)

'Make the main form visible
Gui.frmMain..Visible(True)
F.Intrinsic.Control.CallSub(chkAllWO_Change)

'Set a default report id...
F.Intrinsic.Control.CallSub(LoadDefaultReport)
	
'Retrieve Stored Printer...
F.Intrinsic.Control.CallSub(RetrieveStoredPrinter)

'***********************************************************************
'Set dates to beginning of time and end of time
Gui.frmMain.dtpFrom.Value("1900-01-01")
Gui.frmMain.dtpTo.Value("2199-12-31")
'***********************************************************************

Gui.frmMain.txtJobStart.SetFocus

Program.Sub.Main.End

Program.Sub.frmMain_UnLoad.Start
	F.Intrinsic.Control.Try
		
		F.Intrinsic.Control.CallSub(Serialize)
		F.ODBC.Connection!con.Close
		F.ODBC.Connection!conc.Close
		F.Intrinsic.Control.End
		
	F.Intrinsic.Control.Catch
		F.Intrinsic.Control.CallSub(Catching) 
	F.Intrinsic.Control.EndTry
Program.Sub.frmMain_UnLoad.End

Program.Sub.frmMain_RefreshClick.Start
	F.Intrinsic.Control.Try
		
		F.Intrinsic.Control.If(V.Global.sSQL,<>,"")
			F.Intrinsic.Control.CallSub(DataLoader, "sDataTableName","dtData","sFormName","frmMain","sGridControlName","GsGCData","sGridViewName","gvData","sSQL",V.Global.sSQL,"bSuspendLayout",True)
		F.Intrinsic.Control.EndIf
		
	F.Intrinsic.Control.Catch
		F.Intrinsic.Control.CallSub(Catching) 
	F.Intrinsic.Control.EndTry
Program.Sub.frmMain_RefreshClick.End

Program.Sub.DataLoader.Start
	F.Intrinsic.Control.Try
	
	V.Local..BulkDeclareString(sFieldNames,sColumns)
	V.Local..BulkDeclareLong(iCnt)
	
	F.Intrinsic.Control.BlockEvents
	
	Gui.frmMain.GsGCData.InvokeWait("Loading...","Please Wait",False)
	
	F.Intrinsic.Control.If(V.DataTable.dtData.Exists)
		F.Data.DataTable.Close("dtData")
	F.Intrinsic.Control.EndIf
	
	F.Data.DataTable.CreateFromSQL("dtData","con",V.Global.sSQL,True)
	
	F.Intrinsic.Control.If(V.DataTable.dtData.RowCount,>,0)
		
		F.Intrinsic.Control.If(V.Screen.frmMain!chkLblQty1.CheckedAsBoolean)
			F.Data.DataTable.SetValue("dtData",-1,"LABEL_COPY",1)
		F.Intrinsic.Control.Else
			F.Data.DataTable.SetValue("dtData",-1,"QTY_ON_LABEL",1)
		F.Intrinsic.Control.EndIf
		
		F.Data.DataView.Create("dtData","dvData")
		
		F.Intrinsic.Control.CallSub(LoadGV,"DT","dtData","GV","gvData","GSGC","GsGcData","FRM","frmMain")
		
		'Set Editable Column(s)
		Gui.frmMain.GsGCData.SetColumnProperty("gvData","SELECT","AllowEdit",True)
		Gui.frmMain.GsGCData.SetColumnProperty("gvData","SELECT","ReadOnly",False)
		
