Program.Sub.cmdPrintLabels_Click.Start
	F.Intrinsic.Control.Try
	
	V.Local..BulkDeclareString(sMsg,sPartList,sComma,sSQLPartList,sSQL,sFilename,sReportPath)
	V.Local..BulkDeclareLong(iBIRunID,iBILogID,iBIRptID,iCnt,iUser)
	V.Local..BulkDeclareBoolean(bExists)
	
	F.Intrinsic.Control.If(V.Screen.frmMain!ddlPrinter.Text.Trim,=,"")
		F.Intrinsic.UI.Msgbox("Please select a printer.","Select a Printer")
		Gui.frmMain.ddlPrinter.SetFocus
		F.Intrinsic.Control.ExitSub
	F.Intrinsic.Control.EndIf
	
	'Report Details...
	F.Intrinsic.String.Build("{0}\LABELS\Custom\GAB_6948_MultipleWOLabel.lab",V.Caller.BusintDir,V.Local.sReportPath)
	F.Intrinsic.File.Exists(V.Local.sReportPath,V.Local.bExists)
	F.Intrinsic.Control.If(V.Local.bExists)
		F.Global.BI.InitializeReport("Work Order Label (6948)",V.Local.sReportPath,V.Local.iBIRptID)
	F.Intrinsic.Control.Else
		F.Intrinsic.String.Build("Please ensure that the label template exists.{0}{0}{1}",V.Ambient.NewLine,V.Local.sReportPath,V.Local.sMsg)
		F.Intrinsic.UI.Msgbox(V.Local.sMsg,"Label Template Not Found")
		F.Intrinsic.Control.ExitSub
	F.Intrinsic.Control.EndIf
	
	F.Intrinsic.Control.If(V.DataTable.dtLabel.Exists)
		F.Data.DataTable.Close("dtLabel")
	F.Intrinsic.Control.EndIf
	F.Data.DataView.Create("dtData","dvLabel",22,"SELECT=1","Part Asc")
	F.Data.DataView.ToDataTable("dtData","dvLabel","dtLabel",True)
	F.Data.DataView.Close("dtData","dvLabel")
	F.Data.DataTable.RemoveColumn("dtLabel","@PART")
	F.Data.DataTable.RemoveColumn("dtLabel","@WO")
	
	F.Intrinsic.Control.If(V.DataTable.dtLabel.RowCount,<=,0)
		F.Intrinsic.UI.Msgbox("Please select at least one row.","No Rows Selected")
		F.Intrinsic.Control.ExitSub
	F.Intrinsic.Control.EndIf
	
	'Grab selected Report ID
	V.Local.iBIRptID.Set(V.Screen.frmMain!txtReportID.Text.Long)
	
	'Getting Run ID...
	F.Global.BI.GetRunID(V.Local.iBIRunID)
	'Getting Log ID, start logging...
	F.Global.BI.StartLogging(V.Local.iBIRunID,V.Local.iBIRptID,-1,"",V.Local.iBILogID)
	
	F.Intrinsic.Control.If(V.DataTable.dtLabel.RowCount,>,0)
	    'Printing Codesoft label
	    F.Global.BI.PrintCodesoftLabelFromDataTable(V.Local.iBIRunID,V.Local.iBILogID,"dtLabel",V.Screen.frmMain!ddlPrinter.Text,True)
	F.Intrinsic.Control.Else
		F.Intrinsic.UI.Msgbox("No records found.","Notice")
	F.Intrinsic.Control.EndIf
	
	'Stopping Logging
	F.Global.BI.StopLogging(V.Local.iBILogID)
	
	F.Data.DataTable.Close("dtLabel")
	
	F.Intrinsic.Control.CallSub(SaveSelectedPrinter)
	
	F.Intrinsic.Control.Catch
		F.Intrinsic.Control.CallSub(Catching) 
	F.Intrinsic.Control.EndTry
Program.Sub.cmdPrintLabels_Click.End

